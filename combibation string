#include <stdio.h>
#include <string.h>

int main(void) {
  char name[] = "Algoritma,3";

  //çıktımız da virgülden sonraki sayının önemi olmadığı için her giriş algoritmasını
  //eşit olarak kabul edersem

  printf("%c", *name);  
   printf("%c", *(name+1)); 
  printf("%c", *(name+2));
  printf("%c", *(name+4));
  printf("%c", *(name+5));
  printf("%c", *(name+6));
  printf("%c", *(name+7)); 
  printf("%c\n",*(name+8));
  
 
 
   
  

   char *namePtr;

   namePtr = name;
   printf("%c", *(name));
   printf("%c", *(name+1));
   printf("%c", *(name+2));
   printf("%c", *(name+3));
   printf("%c", *(name+4));
   printf("%c", *(name+6));
   printf("%c", *(name+7));
   printf("%c\n", *(name+(8)));
   
   
  
   
   
  



   char *namePtrs;

   namePtrs = name;
   printf("%c", *(name));
   printf("%c", *(name+1));
   printf("%c", *(name+2));
   printf("%c", *(name+3));
   printf("%c", *(name+4));
   printf("%c", *(name+5));
   printf("%c", *(name+6));
   printf("%c", *(name+7));
   printf("%c\n", *(name+(8)));
   
  
  
 
   

  char *namePtrss;

   namePtrss = name;

   printf("%c", *(name+1));
   printf("%c", *(name+2));
   printf("%c", *(name+3));
   printf("%c", *(name+4));
   printf("%c",  *(name+5));
   printf("%c",  *(name+6));
   printf("%c",  *(name+7));
   printf("%c\n",  *(name+(8)));
   
   


   
   
}
  
